 --restart always \
 -p 3306:3306 \
 -v /docker/zabbix/mysql/data:/var/lib/mysql \
 -e MYSQL_ROOT_PASSWORD=5V%xeBzSHWBd8rMFy@ \
 -e MYSQL_DATABASE=zabbixdb \
